-- <%=dbname%> build script
-- Created with generator-pgsql
-- Build date: <%=now%>

--
-- Database
--
<%=databasescript%>

<% if (useschemas) { %>--
-- Schemas
--

<% _.forEach(schemascripts, function(schemascript) {%>
-- Schema: <%=schemascript.name%>
<%=schemascript.sql%>
<% })} %>

<% if (usetables) { %>--
-- Table creation
--

<% _.forEach(tablescripts, function(tablescript) {%>
-- Table: <%=tablescript.name%>
<%=tablescript.sql%>
<% })} %>

<% if (usesequences) { %>--
-- Sequence creation
--

<% _.forEach(sequencescripts, function(sequencescript) {%>
-- Sequence: <%=sequencescript.name%>
<%=sequencescript.sql%>
<% })} %>

<% if (useviews) { %>--
-- View creation
--

<% _.forEach(viewscripts, function(viewscript) { %>
-- View: <%=viewscript.name%>
<%=viewscript.sql%>
<% })} %>

<% if (usefunctions) { %>--
-- Function creation
--

<% _.forEach(functionscripts, function(functionscript) { %>
-- Function: <%=functionscript.name%>
<%=functionscript.sql%>
<% })} %>
